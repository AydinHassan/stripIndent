<?xml version="1.0" encoding="UTF-8"?>

<phpunit colors="true"
         bootstrap="vendor/autoload.php"
>
    <testsuite name="Strip Ident">
        <directory>./test</directory>
    </testsuite>
    <filter>
        <whitelist addUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./src</directory>
        </whitelist>
    </filter>
</phpunit>
